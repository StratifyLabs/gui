      "background_color": "$colorBackground",
      "border_color": "$colorGrey",
      "border_width": "$sizeCardBorderWidth",
      "text_color": "$colorForeground",
      "border_post": true,
      "horizontal_padding": 0,
      "vertical_padding": 0,
      "row_padding": 0,
